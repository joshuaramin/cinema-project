{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "importHelpers": true,
        "noEmit": true,
        "lib": [
            "ES2023"
        ],
        "target": "ESNext",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "pretty": true,
        "skipLibCheck": true,
        "strict": false,
        "paths": {
            "@/*": ["./*"]
        }
    },
}